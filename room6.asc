function loadClara()
{
  cEgo.x = 154;
  cEgo.y = 154;
  cEgo.FaceDirection(eDirectionUp); 
}

function loadCarlos()
{
  cCarlos.x = 85;
  cCarlos.y = 138;
  cCarlos.ChangeRoom(player.Room);
  cCarlos.LockView(CARLOSBATHROOM_1204);
}

function loadJoanna()
{
}

function loadQueenie()
{
